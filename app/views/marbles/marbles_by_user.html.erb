<h1>Marbles by User</h1>
<br>
<br>
<table class="table">
  <thead>
    <tr>
      <th> User Name</th>
      <th> Marble Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marble.each do |m| %>
      	<tr>
        <td><%= m.user.user_name %></td>
        <td><%= m.marble_name %></td>
        </tr>
    <% end %>
  </tbody>
</table>
<br>

<strong><%= link_to 'New Marble', new_marble_path %><strong>



